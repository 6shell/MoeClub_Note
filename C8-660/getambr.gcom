opengt
 set com 115200n81
 set comecho on
 set senddelay 0.02
 waitquiet 0.2 0.2
 flash 0.1

:start
 send "AT+QNWCFG=\"nr5g_ambr\"^m"
 get 1 "^m" $s
 get 1 ":" $s
 get 1 "," $s
 get 1 "\"" $a
 get 1 "," $q
 get 1 "," $d
 get 1 "," $b
 get 1 "^m" $u
 print "{\"APN\":\"",$a,"\",\"DL\":",$d,",\"UL\":",$u,",\"QCI_DL\":",$q,",\"QCI_UL\":",$b,"}"

:continue
 exit 0
